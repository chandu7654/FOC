#include <stdio.h>

int main() {
    int n, digit, reverse = 0;

    printf("Enter a positive integer: ");
    scanf("%d", &n);

   
    while (n != 0) {
        digit = n % 10;
        reverse = (reverse * 10) + digit;
        n /= 10;
    }

    printf("The reverse of the number is: %d", reverse);

    return 0;
}
